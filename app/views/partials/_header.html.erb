<% if logged_in? %>
  <%= link_to "Profile", user_path(@user), :class => 'navbar-link' %> | 
  <%= button_to "Logout", session_path(current_user.id), method: "delete", :class => 'navbar-link' %>
  Logged in as: <strong><%= current_user.email %></strong>(<%= current_user.user_type %>)  
<% else %>
  <%= link_to "Sign up", new_user_path, :class => 'navbar-link' %> |
  <%= link_to "Login", new_session_path, :class => 'navbar-link' %>
<% end %>




