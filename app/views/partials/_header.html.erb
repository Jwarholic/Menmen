<% if logged_in? %>
        Logged in as: <strong><%= current_user.email %></strong>
        <%= link_to "Logout", session_path, method: :delete, :class => 'navbar-link'  %>
      <% else %>
        <%= link_to "Sign up", new_user_path, :class => 'navbar-link'  %> |
        <%= link_to "Login", new_session_path, :class => 'navbar-link'  %>
      <% end %>

    <!-- User authentican -->
    <% if notice %>
      <p class="alert alert-success"><%= notice %></p>
      <% end %>
      <% if alert %>
        <p class="alert alert-danger"><%= alert %></p>
    <% end %>